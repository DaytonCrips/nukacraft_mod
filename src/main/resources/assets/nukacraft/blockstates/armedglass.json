{
  "multipart": [
    {
      "apply": {
        "model": "nukacraft:block/armedglass_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_noside"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_noside_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "nukacraft:block/armedglass_noside",
        "y": 270
      }
    }
  ]
}